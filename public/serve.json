{
  "headers": [
    {
      "source" : "**/*.@(jpg|jpeg|gif|png|webp)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=31536000"
      }]
    },
    {
      "source" : "**/*.@(js|css)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-cache"
      }]
    }, 
    {
      "source" : "404.html",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "max-age=300"
      }]
    }
  ]
}